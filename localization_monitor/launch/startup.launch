<launch>
  <node pkg="localization_monitor" type="localization_monitor" name="localization_monitor" output="screen">

	<param name="uncertainty_thres" value="0.15"/>
	<param name="score_thres" value="0.3"/>
	<param name="inflation_thres" value="0.2"/>
	<param name="max_lim" value="3.0"/>
	<param name="min_valid_points" value="20"/>

  	<param name="time_limit_secs" value="600.0"/>
	<param name="rate_thres" value="0.6"/>	

  </node>
</launch>

